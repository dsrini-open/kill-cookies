{
  "applications": {
    "gecko": {
      "id": "kill-cookies@dsrini-open.com",
      "strict_min_version": "53"
    }
  },
  "author": "dsrini.open",
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "32": "icons/kill-cookies.png"
    },
    "default_popup": "popup/kill-cookies.html",
    "default_title": "__MSG_extensionName__"
  },
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/dsrini-open/kill-cookies",
  "icons": {
    "32": "icons/kill-cookies.png"
  },
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "options_ui": {
    "browser_style": true,
    "page": "option/option.html"
  },
  "permissions": [
    "browsingData",
    "cookies",
    "storage",
    "<all_urls>",
    "tabs"
  ],
  "version": "1.0"
}